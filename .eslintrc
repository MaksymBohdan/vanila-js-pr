{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "linebreak-style": ["error", "windows"],
    "require-jsdoc": "off",
    "object-curly-spacing": ["error", "always"],
    "operator-linebreak": "off",
    "no-debugger": "off",
    "indent": "off"
  },
  "extends": ["eslint:recommended", "google"]
}
